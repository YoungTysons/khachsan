<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="1000.0" spacing="10.0" style="-fx-background-color: #f4f4f4;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.qlykhsan.NguoiDung.DatPhong">
    <children>
        <VBox>
            <children>
                <Label text="Danh Sách Phòng Trống">
                    <font>
                        <Font name="System Bold" size="22.0" />
                    </font>
                    <padding>
                        <Insets bottom="15.0" left="20.0" top="15.0" />
                    </padding>
                </Label>
            </children>
        </VBox>

        <ScrollPane fitToWidth="true">
            <content>
                <FlowPane fx:id="flowPanePhong" hgap="25.0" vgap="25.0">
                    <padding>
                        <Insets bottom="30.0" left="30.0" right="30.0" top="10.0" />
                    </padding>
                </FlowPane>
            </content>
        </ScrollPane>
    </children>
</VBox>