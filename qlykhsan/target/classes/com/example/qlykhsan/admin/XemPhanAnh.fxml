<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.qlykhsan.admin.XemPhanAnh">
    <children>
        <VBox spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
            <children>
                <Label text="Xem Phản Ánh Từ Khách Thuê">
                    <font>
                        <Font name="System Bold" size="20.0" />
                    </font>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </Label>

                <TableView fx:id="tablePhanAnh" VBox.vgrow="ALWAYS">
                    <columns>
                        <TableColumn fx:id="colNguoiGui" prefWidth="150.0" text="Người gửi" />
                        <TableColumn fx:id="colTieuDe" prefWidth="200.0" text="Tiêu đề" />
                        <TableColumn fx:id="colNoiDung" prefWidth="400.0" text="Nội dung" />
                        <TableColumn fx:id="colNgayGui" prefWidth="120.0" text="Ngày gửi" />
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                    </columnResizePolicy>
                </TableView>
            </children>
        </VBox>
    </children>
</AnchorPane>